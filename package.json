{
  "name": "oldworld-xml-viewer",
  "version": "1.0.0",
  "description": "My tremendous Nuxt.js project",
  "author": "km058",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "generate": "nuxt-ts generate",
    "start": "nuxt-ts start",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore .",
    "xml": "ts-node ./task/parse-xml.ts",
    "parseAPI": "openapi-generator-cli generate --enable-post-process-file -i ./openapi.yaml -t ./template -o ./api --generator-name typescript-axios -c ./openapi-config.yaml"
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@nuxt/typescript-runtime": "^0.4.0",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/pwa": "^3.0.0-0",
    "@openapitools/openapi-generator-cli": "^2.2.6",
    "bulma": "^0.9.2",
    "bulmaswatch": "^0.8.1",
    "case": "^1.6.3",
    "glob": "^7.1.6",
    "json-format": "^1.0.1",
    "json-to-ts": "^1.7.0",
    "nuxt": "^2.0.0",
    "nuxt-property-decorator": "^2.7.2",
    "v-tooltip": "^2.1.3",
    "vuex-module-decorators": "^0.17.0",
    "xml2json": "^0.12.0"
  },
  "devDependencies": {
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@nuxt/typescript-build": "^0.6.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/fontawesome": "^1.1.2",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^7.25.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "husky": "^4.0.0",
    "lint-staged": "^10.0.0",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.1",
    "stylelint": "^10.1.0"
  }
}
