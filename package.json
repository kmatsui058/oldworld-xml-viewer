{
  "name": "oldworld-xml-viewer",
  "version": "1.0.0",
  "description": "My tremendous Nuxt.js project",
  "author": "km058",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "start": "nuxt start",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore .",
    "xml": "ts-node ./task/parse-xml.ts -P ./task/",
    "test": "jest",
    "prepare": "husky install",
    "parseAPI": "openapi-generator-cli generate --enable-post-process-file -i ./openapi.yaml -t ./template -o ./api --generator-name typescript-axios -c ./openapi-config.yaml"
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.21.3",
    "@nuxtjs/composition-api": "^0.32.0",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/pwa": "^3.0.0-0",
    "@nuxtjs/sitemap": "^2.4.0",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@types/jest": "^29.5.0",
    "@vue/test-utils": "^1.3.4",
    "algoliasearch": "^4.17.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^29.5.0",
    "bulma": "^0.9.4",
    "bulmaswatch": "^0.8.1",
    "case": "^1.6.3",
    "core-js": "^3.29.1",
    "glob": "^9.3.4",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-transform-stub": "^2.0.0",
    "json-format": "^1.0.1",
    "json-to-ts": "^1.7.0",
    "nuxt": "^2.15.8",
    "ts-jest": "^29.1.0",
    "v-tooltip": "^2.1.3",
    "vue-instantsearch": "^4.8.8",
    "vue-jest": "^3.0.7",
    "vue-json-component": "^0.4.1",
    "vuex-module-decorators": "^2.0.0",
    "xml2json": "^0.12.0"
  },
  "devDependencies": {
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@nuxtjs/eslint-module": "^4.0.2",
    "@nuxtjs/fontawesome": "^1.1.2",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier-vue": "^4.2.0",
    "eslint-plugin-vue": "^9.10.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "node-sass": "^6.0.1",
    "prettier": "^2.8.7",
    "sass-loader": "^10.1.1",
    "stylelint": "^14.16.1"
  }
}
