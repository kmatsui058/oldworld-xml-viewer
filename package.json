{
  "name": "oldworld-xml-viewer",
  "version": "1.0.0",
  "description": "My tremendous Nuxt.js project",
  "author": "km058",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "generate": "nuxt-ts generate",
    "start": "nuxt-ts start",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore .",
    "xml": "ts-node ./task/parse-xml.ts",
    "test": "jest",
